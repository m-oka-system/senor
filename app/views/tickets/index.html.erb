<div class="row">
  <div class="container table-responsive">
    <table class="table table-striped table-bordered table-hover">
      <!-- テーブルのヘッダー部 -->
      <tr class="success">
        <th>インシデントID</th>
        <th>タイトル</th>
        <th>ステータス</th>
        <th >受付日時</th>
        <th >受付者</th>
        <th >担当者者</th>
      </tr>

      <% @ticket.each do |ticket| %>
        <tr>
          <td><%= ticket.id %></td>
          <td><%= link_to ticket.title, customer_store_ticket_path(ticket.store.customer_id, ticket.store_id, ticket.id) %></td>
          <td><%= ticket.status %></td>
          <td><%= l ticket.receive_date, format: :default %></td>
          <td><%= ticket.received_user_id %></td>
          <td><%= ticket.assign_user_id %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>
