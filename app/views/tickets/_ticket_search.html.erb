<div class="col-md-4">
  <%= search_form_for @q do |f| %>

  <div class="form-group">
    <div class="form-inline">
      <%= f.label :store_customer_customer_name_cont, "会社名", class:"control-label" %>
      <%= f.collection_select :store_customer_customer_name_cont, Customer.all, :customer_name, :customer_name, include_blank: true, class:"form-control" %>
    </div>
  </div>

  <div class="form-group">
    <div class="form-inline">
      <%= f.label :store_store_name_cont, "店舗名", class:"control-label" %>
      <%= f.search_field :store_store_name_cont, class:"form-control" %>
    </div>
  </div>

    <div class="form-group">
      <div class="form-inline">
        <%= f.label :title_cont, "タイトル", class:"control-label" %>
        <%= f.search_field :title_cont, class:"form-control" %>
      </div>
    </div>

    <div class="btn-toolbar">
      <div class="btn-group">
        <%= f.submit "検索", class:"btn btn-primary" %>
      </div>
      <div class="btn-group">
        <%= link_to "クリア", tickets_path, class:"btn btn-default" %>
      </div>
    </div>
  <% end %>
</div>
