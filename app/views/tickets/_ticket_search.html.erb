<div class="col-md-4">
  <%= search_form_for @q do |f| %>

    <div class="form-group">
      <div class="form-inline">
        <%= f.label :title_cont, "タイトル", class:"control-label" %>
        <%= f.search_field :title_cont, class:"form-control" %>
      </div>
    </div>

    <div class="btn-toolbar">
      <div class="btn-group">
        <%= f.submit "検索", class:"btn btn-primary" %>
      </div>
      <div class="btn-group">
        <%= link_to "クリア", tickets_path, class:"btn btn-default" %>
      </div>
    </div>
  <% end %>
</div>
